{
    "name": "flowroute-laravel-notification-channels",
    "description": "Flowroute SMS notifications driver for Laravel",
    "keywords": [
        "laravel",
        "notifications",
        "flowroute"
    ],
    "homepage": "https://github.com/deftx/flowroute-laravel-notification-channels",
    "license": "MIT",
    "authors": [
        {
            "name": "Sid K",
            "email": "sid@koomai.net",
            "homepage": "http://koomai.net",
            "role": "Developer"
        },
        {
            "name": "Rob Vella",
            "email": "me@robvella.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/events": ">=5.4.0",
        "illuminate/notifications": ">=5.4.0",
        "illuminate/support": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Flowroute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Flowroute\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
